class Solution {
public:
    int maxSubArray(vector<int>& nums) {
     int msf = INT_MIN,meh = 0;
     for(int i = 0;i <= nums.size()-1;i++){
         meh = meh + nums[i];
         if(meh<nums[i]){
             meh = nums[i];
         }
         if(msf < meh){
             msf = meh;
         }
     } 
     return msf;  
